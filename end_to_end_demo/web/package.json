{
  "name": "upollo-end-to-end-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "startDev": "APP_ENV=dev next dev -p 3001",
    "startLocal": "APP_ENV=local.go_python next dev -p 3001",
    "startLocalNode": "APP_ENV=local.node next dev -p 3001",
    "buildDev": "APP_ENV=dev next build && next export",
    "buildProd": "APP_ENV=prod next build && next export",
    "lint": "next lint",
    "export": "next export",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "ci": "npm run prettier:check && npm run lint"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@upollo/web": "0.2.5",
    "dotenv": "^16.0.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "use-local-storage-state": "^17.2.0"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "prettier": "^2.6.2"
  }
}
